@model IEnumerable<Cool_Co_Fridge_Management.Models.MaintenanceRequest>

<h2>Confirmed Maintenance Requests</h2>

<table class="table">
    <thead>
        <tr>
            <th>Booking ID</th>
            <th>Customer Name</th>
            <th>Address</th>
            <th>Requested Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var request in Model)
        {
            <tr>
                <td>@request.BookingID</td>
                <td>@request.FirstName @request.LastName</td>
                <td>@request.Address</td>
                <td>@request.RequestedDate.ToString("d")</td>
                <td>@request.UserConfirmationStatus</td>
                
            </tr>
        }
    </tbody>
</table>


