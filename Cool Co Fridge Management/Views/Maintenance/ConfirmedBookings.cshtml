@model IEnumerable<Cool_Co_Fridge_Management.Models.MaintenanceRequest>
@{
    ViewData["Title"] = "ConfrmedBooking";
}
<h2>Confirmed Maintenance Requests</h2>

<table class="table">
    <thead>
        <tr>
            <th>Booking ID</th>
            <th>Customer Name</th>
            <th>Confirmed Date</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var booking in Model)
        {
            <tr>
                <td>@booking.BookingID</td>
                <td>@booking.FirstName @booking.LastName</td>
                <td>@booking.RequestedDate.ToString("g")</td>
                <td>@booking.Address</td>
            </tr>
        }
    </tbody>
</table>


